    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var categoryNames = @Html.Raw(Json.Serialize(ViewBag.CategoryNames));
            var productCounts = @Html.Raw(Json.Serialize(ViewBag.ProductCounts));

            var ctx = document.getElementById("myChart").getContext("2d");
            var myChart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: categoryNames,
                    datasets: [{
                        label: 'Produkty w kategorii',
                        data: productCounts,
                        backgroundColor: 'rgba(0, 95, 63, 0.7)',
                        borderColor: 'rgba(0, 95, 63, 1)',
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    scales: {
                        y: {
                            beginAtZero: true,
                            precision: 0
                        }
                    }
                }
            });
        });
    </script>
