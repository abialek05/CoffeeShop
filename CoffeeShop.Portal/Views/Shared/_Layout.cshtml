    <script>
        function myMap() {
            var mapProp = {
                center: new google.maps.LatLng(49.61074309925255, 20.63799864587533),
                zoom: 9,
            };
            var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);

            var marker = new google.maps.Marker({
                position: new google.maps.LatLng(49.61074309925255, 20.63799864587533),
                map: map,
                title: "Zafusowani"
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBFRneEyB8Bb14GArlhwV6Z8rPg-fzbZV4&callback=myMap"></script>
    <script src="~/lib/js/google-map.js"></script>
    <script src="~/lib/js/main.js"></script>    
    @await RenderSectionAsync("Scripts", required: false)
